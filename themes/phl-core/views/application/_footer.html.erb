<footer class="footer container">
  <div class="footer_inner">
    <span>
      <%= DispatchConfiguration.site_title %> is built by
      <a href="http://jarv.us/" target="_blank">Jarvus Innovations</a> and leverages
      <a href="https://github.com/JarvusInnovations/dispatch" target="_blank">Dispatch</a> from the <a href="https://www.dobt.co/">Department of Better Technology</a>.
    </span>
    <ul>
      <% Array(DispatchConfiguration.static_pages).each do |page| %>
        <li>
          <a href="/<%= page['path'] %>"><%= page['name'] %></a>
        </li>
      <% end %>

      <% Array(DispatchConfiguration.footer_links).each do |link| %>
        <li>
          <a href="<%= link['href'] %>"><%= link['text'] %></a>
        </li>
      <% end %>
    </ul>
  </div>
</footer>